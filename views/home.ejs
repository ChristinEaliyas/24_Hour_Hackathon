    <style>
      .countdown-container {
        text-align: center;
      }
      .countdown {
        display: inline-block;
        font-size: 60px;
        text-align: center;
        font-weight: 400;
        margin-right: 20px;
      }
      
      .countdown-number {
        display: block;
        margin-bottom: 10px;
        margin-right: 20px;
      }
      
      .countdown-label {
        font-size: 20px;
        margin-right: 29px;
        text-align: center;
      }
      
      .expires-in {
        font-size: 24px;
        text-align: center;
